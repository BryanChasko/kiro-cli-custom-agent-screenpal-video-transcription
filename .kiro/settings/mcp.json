{
  "mcpServers": {
    "video-transcriber": {
      "command": "node",
      "args": ["/tmp/video-transcriber-mcp/dist/index.js"],
      "env": {
        "WHISPER_MODEL": "base",
        "YOUTUBE_FORMAT": "bestaudio",
        "WHISPER_DEVICE": "cpu"
      },
      "timeout": 300000,
      "disabled": false
    },
    "vision-server": {
      "command": "node",
      "args": ["/tmp/moondream-mcp/build/index.js"],
      "env": {
        "OLLAMA_BASE_URL": "http://localhost:11434"
      },
      "timeout": 180000,
      "disabled": false
    }
  }
}
